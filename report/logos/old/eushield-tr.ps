%% lettering
/upper_y_offset 0.32 def
/lower_y_offset -0.89 def

%f1 [ letter_scale 0 0 letter_scale 0 0 ] makefont setfont
f1 46 scalefont setfont

/letter	{
    /text exch def
    /y_offs exch def
    /x_offs exch def
    /letter_angle exch def
    /position_angle exch def

    gsave
    BlackOrWhite setcolor

    x0 y0 translate		% center of crest

    %% position letter at some angle on the circle around this center
    position_angle rotate
    0 text_radius translate

    %% now rotate the letter around its own origin (usually to flip it)
    letter_angle rotate

    %% apply any tweaking adjustments, and draw it
    x_offs letter_scale mul y_offs letter_scale mul moveto
    text show

    grestore
} def

